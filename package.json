    "karma-systemjs": "^0.9.0"
  },
  "jspm": {
    "dependencies": {
      "angular": "github:angular/bower-angular@^1.4.8",
      "bows": "npm:bows@^1.4.8",
      "jquery": "github:components/jquery@^2.1.4",
      "jwt-decode": "npm:jwt-decode@^1.4.0",
